/*
 * Configuration header generated by cmake.
 */

// Whether there is LLVM support.
#cmakedefine WITH_EXECUTION_ENGINE_LLVM 1

// If LLVM version is >= 2.8 and < 3.0:
#cmakedefine INA_LLVM_VERSION_IS_2X 1
// If LLVM version is  >= 3.0:
#cmakedefine INA_LLVM_VERSION_IS_3X 1
// If LLVM version is >= 3.2
#cmakedefine INA_LLVM_VERSION_IS_32 1

#cmakedefine INA_ENABLE_OPENMP 1
#cmakedefine INA_ENABLE_VERSION_CHECK 1


#define INA_VERSION_MAJOR ${INA_VERSION_MAJOR}
#define INA_VERSION_MINOR ${INA_VERSION_MINOR}
#define INA_VERSION_PATCH ${INA_VERSION_PATCH}
#define INA_VERSION_STRING "${INA_VERSION_MAJOR}.${INA_VERSION_MINOR}.${INA_VERSION_PATCH}"

/*
 * Configuration header generated by cmake.
 */

// Whether there is LLVM support.
#cmakedefine WITH_EXECUTION_ENGINE_LLVM 1

// If LLVM version is >= 2.9 and < 3.0:
#cmakedefine INA_LLVM_VERSION_IS_2X 1
// If LLVM version is  >= 3.0:
#cmakedefine INA_LLVM_VERSION_IS_3X 1

// This flag will be set on all binary distributed iNA version that are updated via
// a package manager i.e. on linux distributions and MacPorts etc. It is not set
// (enableing version check) on all source and binary distributions w/o update mechanism.
#cmakedefine INA_DISABLE_NEW_VERSION_CHECK 1


#define INA_VERSION_MAJOR 0
#define INA_VERSION_MINOR 4
#define INA_VERSION_PATCH 0
#define INA_VERSION_STRING "0.4.0"

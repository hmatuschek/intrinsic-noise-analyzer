<?xml version="1.0" encoding="UTF-8"?>

<sbml xmlns="http://www.sbml.org/sbml/level2/version4" level="2" version="4">
  <model id="const_species">
    <listOfCompartments>
      <compartment id="compartmentOne" size="1"/>
    </listOfCompartments>

    <listOfSpecies>
      <species id="S1" initialConcentration="1" compartment="compartmentOne" constant="true"/>
      <species id="S2" initialConcentration="0" compartment="compartmentOne"/>
    </listOfSpecies>
    
    <listOfParameters>
      <parameter id="t" value = "1" units="second"/>
    </listOfParameters>
    
    <listOfReactions>
      <reaction id="reaction_1" reversible="false">
        <listOfReactants>
          <speciesReference species="S1"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="S2"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply> 
              <times/> 
              <ci> t </ci>
              <ci> S1 </ci>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
    </listOfReactions>
  </model>
</sbml>  